{
  "name": "redux-saga-api",
  "version": "1.1.0",
  "description": "Framework CRUD REST style api function with redux-saga",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "build": "npm run build:commonjs && npm run build:types && npm run build:umd && npm run build:umd:min && npm run build:es",
    "prepublishOnly": "npm run clean && npm run test && npm run build",
    "postpublish": "git add -A package.json src lib es && git commit -m \"publish v1.1.0\" && git tag -a -m \"publish v1.1.0\" v1.1.0 HEAD && git push && git push --tags",
    "posttest": "npm run lint",
    "flow": "flow check",
    "lint": "eslint src test",
    "test": "cross-env BABEL_ENV=commonjs nyc mocha --compilers js:babel-core/register --reporter spec test/*.js",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack",
    "build:types": "node scripts/export-types.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/shengnian/redux-saga-api.git"
  },
  "homepage": "https://shengnian.org",
  "keywords": [
    "redux",
    "crud",
    "SPA",
    "fetch",
    "whatwg-fetch",
    "restful",
    "rest api",
    "models",
    "store"
  ],
  "author": "C <c@shengnian.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.3",
    "deep-freeze": "0.0.1",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "estraverse-fb": "^1.3.2",
    "expect": "^1.20.2",
    "flow": "^0.2.3",
    "flow-bin": "^0.52.0",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "recursive-copy": "^2.0.6",
    "rimraf": "^2.6.1",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "global": "^4.3.2",
    "istanbul": "^0.4.5",
    "lodash.isequal": "^4.5.0",
    "redux-saga": "^0.15.6",
    "whatwg-fetch": "^2.0.3"
  }
}
